esheet\r\n    const anims = scene.anims;\r\n    anims.create({\r\n      key: \"johnny-idle\",\r\n      frames: anims.generateFrameNumbers(\"johnny\", { start: 0, end: 3 }),\r\n      frameRate: 3,\r\n      repeat: -1\r\n    });\r\n    anims.create({\r\n      key: \"johnny-walk\",\r\n      frames: anims.generateFrameNumbers(\"johnny\", { start: 5, end: 7 }),\r\n      frameRate: 12,\r\n      repeat: -1\r\n    });\r\n\r\n    function moveTowards(targetX, targetY) {\r\n      if (this.x > targetX) {\r\n        this.x -= 1;\r\n      } else {\r\n        this.x += 1;\r\n      }\r\n      if (this.y > targetY) {\r\n        this.y -= 1;\r\n      } else {\r\n        this.y += 1;\r\n      }\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"6e516f59858036063410435d8254dd52","cacheData":{"env":{}}}